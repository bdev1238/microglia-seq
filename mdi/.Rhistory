df$tx <- factor(df$tx, levels = c('SAL', 'LPS'))
sem <- function(x) sqrt(var(x)/length(x))
df_grouped <- df %>%
group_by(age, sex, tx) %>%
summarise(sem = sem(index), index = mean(index))
ggplot(df_grouped, aes(x = interaction(age,tx), y = index, fill = interaction(sex, age)))+
geom_bar(stat = 'identity', position = position_dodge(.9))+
geom_errorbar(aes(ymin = index - sem, ymax = index + sem), color = 'black', width = .2, size = 1.2, position=position_dodge(.9))+
stat_compare_means(data = df, method = 't.test', aes(group = sex), label = 'p.format')+
theme_classic()
ggplot(df_grouped, aes(x = interaction(age,tx), y = index, color = sex, shape = sex), fill = interaction(sex, age))+
geom_point(data = df, aes(x = interaction(age,tx), y = index, color = sex), size = 4, position = position_dodge(.5))+
geom_errorbar(aes(ymin = index - sem, ymax = index + sem), color = 'black', width = .2, size = 1.2, position=position_dodge(.5))+
stat_compare_means(data = df, method = 't.test', aes(group = sex), label = 'p.format')+
ggtitle('Index created from Dr. Alters ALL data file')+
theme_classic()+
